{
  "lerna": "2.9.1",
  "packages": [
    "packages/*"
  ],
  "command": {
    "publish": {
      "ignoreChanges": [
        "ignored-file",
        "*.md"
      ],
      "message": "chore(release): publish"
    },
    "bootstrap": {
      "ignore": "component-*",
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  },
  "changelog": {
    "repo": "littleants707/royplayer",
    "labels": {
      "player": "royplayer",
      "mp4": "royplayer-mp4",
      "hls": "royplayer-hls",
      "hls.js": "royplayer-hls.js",
      "flv": "royplayer-flv",
      "flv.js": "royplayer-flv.js",
      "dash": "royplayer-shaka",
      "logger": "royplayer-logger",
      "music": "royplayer-music"
    },
    "cacheDir": ".changelog"
  },
  "version": "0.0.3"
}
